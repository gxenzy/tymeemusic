{
  "name": "yukihana-discord-bot",
  "version": "2.0.0",
  "description": "A versatile Discord.js bot with advanced sharding, emoji management, and web dashboard",
  "type": "module",
  "main": "src/index.js",
  "scripts": {
    "start": "node --max-old-space-size=512 --optimize_for_size --gc_interval=100 src/shard.js",
    "start:perf": "node --max-old-space-size=256 --optimize_for_size --gc_interval=50 --expose-gc src/shard.js",
    "dev": "node --watch src/shard.js",
    "test": "node --test",
    "setup": "cp .env.example .env && echo 'Created .env file. Please edit it with your configuration.'",
    "dashboard": "node src/web/dashboard.js",
    "deploy": "node src/deploy.js"
  },
  "imports": {
    "#commands/*": "./src/commands/*.js",
    "#structures/*": "./src/structures/*.js",
    "#database/*": "./src/database/*.js",
    "#config/*": "./src/config/*.js",
    "#utils/*": "./src/utils/*.js",
    "#events/*": "./src/events/*.js",
    "#managers/*": "./src/managers/*.js",
    "#handlers/*": "./src/structures/handlers/*.js",
    "#classes/*": "./src/structures/classes/*.js",
    "#db/*": "./src/database/repo/*.js",
    "#web/*": "./src/web/*.js"
  },
  "keywords": [
    "discord",
    "bot",
    "discord.js",
    "sharding",
    "modular",
    "yukihana",
    "music",
    "dashboard",
    "emoji"
  ],
  "author": "Bre4d777",
  "license": "MIT",
  "dependencies": {
    "@napi-rs/canvas": "^0.1.74",
    "axios": "^1.11.0",
    "better-sqlite3": "^11.9.1",
    "chalk": "^5.4.1",
    "compression": "^1.8.1",
    "discord-hybrid-sharding": "^2.2.6",
    "discord.js": "^14.22.0",
    "discord.js-docs": "^0.4.3",
    "dotenv": "^16.6.1",
    "express": "^5.1.0",
    "express-rate-limit": "^7.5.0",
    "express-session": "^1.18.2",
    "fast-json-stringify": "^6.1.1",
    "fs-extra": "^11.3.0",
    "helmet": "^8.0.0",
    "ioredis": "^5.4.1",
    "jsonwebtoken": "^9.0.2",
    "lavalink-client": "^2.5.7",
    "moment": "^2.30.1",
    "moment-duration-format": "^2.3.2",
    "ms": "^2.1.3",
    "node-cache": "^5.1.2",
    "node-fetch": "^2.7.0",
    "p-limit": "^7.2.0",
    "p-queue": "^9.1.0",
    "passport": "^0.7.0",
    "passport-discord": "^0.1.4",
    "quick-lru": "^7.3.0",
    "socket.io": "^4.8.1",
    "undici": "^7.8.0",
    "v8-compile-cache-lib": "^3.0.1",
    "ws": "^8.18.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "eslint-formatter-pretty": "^6.0.1"
  }
}
